<script type="text/javascript">
	//debugger;
	$(function() {
		if (typeof SevenELocator !== 'function') {
			$('form#findStoreForm').on('submit.locatorForm', function() {
				var searchValue = $("#storeLocatorSearch").val(),
					toolTip = $("#storeLocatorSearch").attr("data-tooltip"),
					range = $("#searchRangeMiles").val(),
					isValid = true;

				if (searchValue == toolTip || searchValue == "" || searchValue == null) {
					isValid = false;
					$("#storeLocatorSearch").addClass("validationError");
				} else {
					$("#storeLocatorSearch").removeClass("validationError");
				}
				if (range == null || range == "") {
					isValid = false;
					$("#searchRangeMiles").addClass("validationError");
				} else {
					$("#searchRangeMiles").removeClass("validationError");
				}
				if (isValid) {
					return true;
				} else {
					alert('Please enter a location (Ex: zip code, city, or landmark) and a search radius.');
					return false;
				}

				$('#advancedSearchAnchor').on('click', function() {
					if ($("#advancedSearchAnchor").hasClass("expanded")) {
						$("#advancedSearchAnchor").removeClass("expanded").addClass("collapsed");
						$("#searchSelections").slideUp("slow");
						//$(".advancedSearchContentHolder .advancedSearchContent").slideUp('slow');
					} else {
						$("#advancedSearchAnchor").removeClass("collapsed").addClass("expanded");
						$("#searchSelections").slideDown("slow");
						//$(".advancedSearchContentHolder .advancedSearchContent").slideDown('slow');
					}
				});
			});
		}
	});
</script>

